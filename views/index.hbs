					</div>
					<div class="question">
						{{{question.question}}}
					</div>
					<div class="answers">
						<ol>
							<li>
								<span class="custom-radio">
									<input type="radio"
												 id="radQuestion{{questionIndex}}Yes" value="1"
												 name="{{questionIndex}}">
									<label class="yes" for="radQuestion{{questionIndex}}Yes">Oui</label>
								</span>
							</li>
							<li>
								<span class="custom-radio">
									<input type="radio"
												 id="radQuestion{{questionIndex}}No" value="0"
												 name="{{questionIndex}}">
									<label class="no" for="radQuestion{{questionIndex}}No">Non</label>
								</span>
							</li>
						</ol>
					</div>
				</section>
			{{/each}}
			<input type='reset' class="button-reset"/>
		</form>
	</main>
</div>
{{/if}}
{{#if winner}}
	<div class="final-container winner" style="display:flex" id="final-{{winner.key}}">
		{{> character final=winner url=winner.key}}
	</div>
{{else}}
	{{#each finals as |final|}}
		<div class="final-container" id="final-{{@key}}">
			{{> character final=final url=@key}}
		</div>
	{{/each}}
{{/if}}
